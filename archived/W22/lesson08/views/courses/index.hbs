<h1>{{title}}</h1>
{{#if user}}
    <a href="/courses/add" class="btn btn-primary">Add a new Course</a>
{{/if}}

<table class="table table-striped table-hover">
    <thead>
        <th>Name</th>
    </thead>
    {{#each dataset}}
    <tr>
        <td>{{this.name}}</td>
    </tr>
    {{/each}}
</table>